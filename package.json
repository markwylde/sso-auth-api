{
  "name": "auth-api",
  "version": "1.0.0",
  "description": "An authentication API for user management",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "start:dev": "nodemon -L --polling-interval 500 src",
    "migrate": "migrator up",
    "test:features:watch": "nodemon -q -x 'clear && npm run test:features'",
    "test:features": "tape 'features/**/*.js'",
    "test:units:watch": "nodemon -q -x 'clear && npm run test:units'",
    "test:units": "tape 'tests/**/*.js'",
    "test": "npm run test:features && npm run test:units"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:markwylde/sso-auth-api.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "nodemon": "^1.19.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.11.0"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "ajv-errors": "^1.0.1",
    "axios": "^0.19.0",
    "generic-errors": "^2.1.0",
    "js-yaml": "^3.12.0",
    "node-mini-migrations": "1.0.0",
    "rethinkdbdash": "^2.3.31",
    "route-parser": "0.0.5",
    "uuid": "^3.3.3"
  }
}
